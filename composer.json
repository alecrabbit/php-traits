{
    "name": "alecrabbit/php-traits",
    "type": "library",
    "description": "PHP traits to use in every project :)",
    "keywords": [
        "php",
        "traits",
        "trait",
        "start",
        "stop",
        "name",
        "private"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "kylekatarnls/multi-tester": "^1.1 || ^2.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "AlecRabbit\\": "src"
        },
        "files": [
            "src/Traits/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AlecRabbit\\Tests\\Traits\\": "tests"
        }
    },
    "prefer-stable": true
}
